{
  "compilerOptions": {
    "allowJs": true,
    "jsx": "react",
    "isolatedModules": false,
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
  },
  "include": [
    "src"
  ]
}